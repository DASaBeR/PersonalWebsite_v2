@model IndexVM

<footer id="footer">
	<div class="container">
		<div class="social-links">
			<a href="@Model.PersonalInfo.Telegram" class="telegram"><i class="bi bi-telegram"></i></a>
			<a href="@Model.PersonalInfo.WhatsApp" class="whatsapp"><i class="bi bi-whatsapp"></i></a>
			<a href="@Model.PersonalInfo.Instagram" class="instagram"><i class="bi bi-instagram" aria-hidden="true"></i></a>
			<a href="@Model.PersonalInfo.Github" class="github"><i class="bi bi-github"></i></a>
		</div>
		<div class="copyright">
			All files of this site are open source under the <a href="#">MIT license</a>.<br><a href="@Model.PersonalInfo.Github">Project Files</a>
		</div>
    <div id="music" class="row mt-5">
      @foreach (var item in Model.Musics)
			{
				<div class="col-lg-12 col-md-6">
					<iframe width="100%" height="200" scrolling="no" frameborder="no" allow="autoplay" src="@item.Link"></iframe>
				</div>
			}
    </div>
</footer>